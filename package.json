{
  "name": "ng2-webpack-starter",
  "version": "1.0.0",
  "description": "Angular 2 with Webpack starter",
  "main": "index.js",
  "scripts": {
    "install": "npm install -g tsc && npm update",
    "start": "webpack-dev-server --inline --progress --open --port 8381",
    "w": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ty": "typings install",
    "postinstall": "npm run ty",
    "prebuild": "rimraf dist",
    "build": "webpack --config webpack.prod.js --progress --profile --bail",
    "prestart:dist": "npm run build",
    "start:dist": "http-server dist -a localhost -p 8381 "
  },
  "keywords": [
    "ng2"
  ],
  "author": "Andrei Antal",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "2.0.0",
    "@types/node": "^10.12.18",
    "angular2-jwt": "0.1.24",
    "angular2-recaptcha": "0.3.3",
    "bootstrap-sass": "3.3.7",
    "config": "1.21.0",
    "core-js": "2.4.1",
    "font-awesome": "4.6.3",
    "ng2-bootstrap": "1.1.1",
    "ng2-select": "1.1.0",
    "ng2-validation": "2.0.0",
    "rxjs": "5.0.0-beta.12",
    "webpack": "^3.10.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^2.9.7",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "angular2-template-loader": "0.5.0",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "http-server": "0.9.0",
    "node-sass": "3.9.3",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "typescript": "2.0.3",
    "typings": "1.3.3"
  }
}
